<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="homeview" prefHeight="389.0" prefWidth="544.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.javafxprac.HomeController">
   <children>
      <VBox fx:id="pnHome" alignment="TOP_CENTER" nodeOrientation="LEFT_TO_RIGHT" prefHeight="389.0" prefWidth="544.0">
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="105.0" prefWidth="418.0" styleClass="blue" text="WELCOME TO OUR HOME PAGE DEAR USER: " textAlignment="RIGHT">
               <font>
                  <Font name="GDT" size="15.0" />
               </font>
               <effect>
                  <Bloom />
               </effect>
            </Label>
            <GridPane fx:id="pnLogOut">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Button alignment="CENTER" mnemonicParsing="false" onAction="#onSignOutClick" prefHeight="43.0" prefWidth="81.0" text="Sign Out" textAlignment="RIGHT" textOverrun="WORD_ELLIPSIS" GridPane.columnIndex="1">
                     <font>
                        <Font name="GothicG" size="12.0" />
                     </font>
                  </Button>
                  <ColorPicker fx:id="cpPicker" />
               </children>
            </GridPane>
         </children>
      </VBox>
   </children>
</AnchorPane>
